{
  "Name": "vismotion",
  "BIDSModelVersion": "1.0.0",
  "Description": "model with no actual condition in design matrix, to use GLM fo denoising only",
  "Input": {
    "task": [
      "vismotion"
    ],
    "space": [
      "IXI549Space"
    ]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "run_level",
      "GroupBy": [
        "run",
        "subject"
      ],
      "Model": {
        "Type": "glm",
        "X": [
          "trans_?",
          "rot_?"
        ],
        "HRF": {
          "Variables": [
            "trial_type.VisMot",
            "trial_type.VisStat"
          ],
          "Model": "DoubleGamma"
        },
        "Options": {
          "HighPassFilterCutoffHz": 0.008
        },
        "Software": [
          {
            "SPM": {
              "SerialCorrelation": "FAST",
              "HRFderivatives": "Temporal"
            }
          }
        ]
      }
    }
  ]
}
