{
  "Name": "Motion localizer",
  "Description": "contrasts for the motion localizer dataset",
  "Input": {
    "task": "visMotion",
    "space": "IXI549Space"
  },
  "Nodes": [
    {
      "Level": "run",
      "Name": "run_level",
      "Model": {
        "X": [
          "trial_type.VisMot",
          "trial_type.VisStat",
          "trans_?",
          "rot_?",
          "non_steady_state_outlier*",
          "motion_outlier*"
        ],
        "HRF": {
          "Variables": [
            "trial_type.VisMot",
            "trial_type.VisStat"
          ],
          "Model": "DoubleGamma"
        },
        "Type": "glm",
        "Options": {
          "HighPassFilterCutoffHz": 0.0078,
          "Mask": ""
        },
        "Software": {
          "SPM": {
            "SerialCorrelation": "FAST",
            "HRFderivatives": "Temporal"
          }
        }
      }
    },
    {
      "Level": "subject",
      "Model": {
        "X": [
          "trial_type.VisMot",
          "trial_type.VisStat",
          "trans_?",
          "rot_?",
          "non_steady_state_outlier*",
          "motion_outlier*"
        ],
        "HRF": {
          "Variables": [
            "trial_type.VisMot",
            "trial_type.VisStat"
          ],
          "Model": "DoubleGamma"
        },
        "Type": "glm",
        "Options": {
          "HighPassFilterCutoffHz": 0.0078,
          "Mask": ""
        },
        "Software": {
          "SPM": {
            "SerialCorrelation": "FAST",
            "HRFderivatives": "Temporal"
          }
        }
      },
      "DummyContrasts": {
        "Test": "t",
        "Contrasts": [
          "trial_type.VisMot",
          "trial_type.VisStat"
        ]
      },
      "Contrasts": [
        {
          "Name": "VisMot_gt_VisStat",
          "ConditionList": [
            "trial_type.VisMot",
            "trial_type.VisStat"
          ],
          "Weights": [
            1,
            -1
          ],
          "Test": "t"
        },
        {
          "Name": "VisStat_gt_VisMot",
          "ConditionList": [
            "trial_type.VisMot",
            "trial_type.VisStat"
          ],
          "Weights": [
            -1,
            1
          ],
          "Test": "t"
        }
      ]
    },
    {
      "Level": "Dataset",
      "Name": "dataset_level",
      "Model": {
        "X": [
          " "
        ],
        "Type": "glm",
        "Options": {
          "Mask": ""
        }
      },
      "DummyContrasts": {
        "Test": "t",
        "Contrasts": [
          "trial_type.VisMot",
          "trial_type.VisStat",
          "VisMot_gt_VisStat",
          "VisStat_gt_VisMot"
        ]
      }
    }
  ]
}
