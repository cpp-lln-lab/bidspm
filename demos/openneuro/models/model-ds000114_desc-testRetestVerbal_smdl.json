{
  "Name": "Model for ds000114 cross session test-retest",
  "BIDSModelVersion": "1.0.0",
  "Description": "BIDS stats model for linebisection / oververbgeneration / overwordrepetition / covertverbgeneration tasks",
  "Input": {
    "task": [
      "overtverbgeneration",
      "overtwordrepetition",
      "covertverbgeneration"
    ],
    "space": ["MNI152NLin2009cAsym"]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "run",
      "GroupBy": [
        "run",
        "session",
        "subject"
      ],
      "Model": {
        "X": [
          "trial_type.Task",
          "trans_?",
          "rot_?",
          "non_steady_state_outlier*",
          "motion_outlier*",
          "1"
        ],
        "Type": "glm",
        "HRF": {
          "Variables": [
            "trial_type.Task"
          ],
          "Model": "spm"
        },
        "Options": {
          "HighPassFilterCutoffHz": 0.008,
          "Mask": {
            "desc": ["brain"],
            "suffix": ["mask"]
          }
        },
        "Software": {
          "SPM": {
            "SerialCorrelation": "FAST",
            "InclusiveMaskingThreshold": 0.8
          }
        }
      },
      "DummyContrasts": {
        "Test": "t",
        "Contrasts": [
          "trial_type.Task"
        ]
      }
    },
    {
      "Level": "Subject",
      "Name": "subject",
      "GroupBy": [
        "subject",
        "contrast"
      ],
      "Model": {
        "X": [1],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    },
    {
      "Level": "Dataset",
      "Name": "dataset",
      "GroupBy": ["contrast"],
      "Model": {
        "X": [1],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    }
  ],
  "Edges": [
    {
      "Source": "run",
      "Destination": "subject"
    },
    {
      "Source": "subject",
      "Destination": "dataset"
    }
  ]
}
